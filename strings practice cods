#include<stdio.h>
char* my_string(char *s1, char *s2){
    int i;
    for(i=0; s1[i]!=0; i++){
        s2[i]=s1[i];
    }
    s2[i]='\0';
    
    return s2;
}
int main(){
    char str1[100];
    char str2[100];
    printf("enter string: ");
    scanf("%[^\n]", str1);
    getchar();
    char *ret=my_string(str1, str2);
    printf("the copied string is: %s\n", ret);
}
