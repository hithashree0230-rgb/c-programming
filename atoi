#include <stdio.h>
int my_atoi(const char str[])
{
    int i=0;
    int num=0;
    int sign=1;
    if(str[i]=='-')
    {
        sign=-1;
        i++;
    }
    else if(str[i]=='+')
    {
        i++;
    }
    else if(str[i]=='+')
    {
        i++;
    }
    while(str[i]!='\0')
    {
        if(str[i]>'0' && str[i]<='9')
        {
            num=num*10+(str[i]-'0');
        }
        else
        {
            break;
        }
        i++;
    }
    return num*sign;
}

int main()
{
    char str[100];
    
    printf("Enter a numeric string : ");
    scanf("%s", str);
    
    printf("String to integer is %d\n", my_atoi(str));
}
