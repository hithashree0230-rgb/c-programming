#include<stdio.h>
int main(){
    int size, i,count=0;
    printf("Enter the array size: ");
    scanf("%d", &size);
    int arr[size];
    printf("Enter the array elements: ");
    for(i=0; i<size;i++){
        scanf("%d", &arr[i]);
    }
    int max=arr[0];
    for(i=0;i<size; i++){
        if(arr[i]>max)
        max=arr[i];
    }
    printf("Largest element is: %d", max);
     return 0;
}
